<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
   assembly = "ORM_NHibernate" namespace = "ORM_NHibernate.BusinessObjects">

  <class name = "OnepAmptp" lazy="true" table="onep_amptp" >
    <id name="Id" column="oid">
      <generator class="foreign">
        <!--copies the Id in a one-to-one rel-->
        <param name="property">OnepTerminationpoint</param>
      </generator>
    </id>
    
    <property name="TargetGain" column="targetGain" />
    <one-to-one name="OnepTerminationpoint" class="OnepTerminationpoint" constrained="true" foreign-key="none"/>
    <many-to-one name="OnepNetwork" column="network" class="OnepNetwork" />
  </class>

</hibernate-mapping>
