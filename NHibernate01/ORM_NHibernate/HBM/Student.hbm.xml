<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
   assembly = "ORM_NHibernate" namespace = "ORM_NHibernate">

  <class name = "Student">
    <id name = "Id">
      <generator class = "identity"/>
    </id>

    <property name = "Lastname"/>
    <property name = "Firstname"/>
  </class>

</hibernate-mapping>
